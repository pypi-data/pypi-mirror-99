    },
    "DescribeFlowServiceDetail": {
      "document": "查询该用户指定状态机下的详情数据。",
      "input": "DescribeFlowServiceDetailRequest",
      "name": "查询状态机详情",
      "output": "DescribeFlowServiceDetailResponse"
    },
    "DescribeFlowServices": {
      "document": "查询指定用户下所有状态机，以列表形式返回",
