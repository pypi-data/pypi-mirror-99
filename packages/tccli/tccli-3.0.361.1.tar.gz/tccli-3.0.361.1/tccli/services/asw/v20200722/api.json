      "name": "查询状态机列表",
      "output": "DescribeFlowServicesResponse"
    },
    "ModifyFlowService": {
      "document": "该接口用于修改状态机",
      "input": "ModifyFlowServiceRequest",
      "name": "修改状态机",
      "output": "ModifyFlowServiceResponse"
    },
    "StartExecution": {
      "document": "为指定的状态机启动一次执行",
      "input": "StartExecutionRequest",
      "name": "启动执行",
      "output": "StartExecutionResponse"
    }
  },
