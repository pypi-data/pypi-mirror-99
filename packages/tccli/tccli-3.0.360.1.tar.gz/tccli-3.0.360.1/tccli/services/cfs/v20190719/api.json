  "metadata": {
    "apiVersion": "2019-07-19",
    "api_brief": "介绍如何使用API对文件存储进行操作，包括对文件系统、权限组等的创建、删除、修改等操作。",
    "serviceNameCN": "文件存储",
    "serviceShortName": "cfs"
  },
  "objects": {
    "AvailableProtoStatus": {
      "document": "版本控制-协议详情\n",
      "members": [
        {
          "default": "",
          "document": "售卖状态。可选值有 sale_out 售罄、saling可售、no_saling不可销售",
          "member": "string",
          "name": "SaleStatus",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "协议类型。可选值有 NFS、CIFS",
          "member": "string",
          "name": "Protocol",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        }
      ],
      "usage": "out"
    },
    "AvailableRegion": {
      "document": "版本控制-区域数组",
      "members": [
        {
          "default": "",
          "document": "区域名称，如“ap-beijing”",
          "member": "string",
          "name": "Region",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "区域名称，如“bj”",
          "member": "string",
          "name": "RegionName",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "区域可用情况，当区域内至少有一个可用区处于可售状态时，取值为AVAILABLE，否则为UNAVAILABLE",
          "member": "string",
          "name": "RegionStatus",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "可用区数组",
          "member": "AvailableZone",
          "name": "Zones",
          "required": true,
          "type": "list",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "区域中文名称，如“广州”",
          "member": "string",
          "name": "RegionCnName",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        }
      ],
      "usage": "out"
    },
    "AvailableType": {
      "document": "版本控制-类型数组",
      "members": [
        {
          "default": "",
          "document": "协议与售卖详情",
          "member": "AvailableProtoStatus",
          "name": "Protocols",
          "required": true,
          "type": "list",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "存储类型。返回值中 SD 为标准型存储、HP 为性能型存储",
          "member": "string",
          "name": "Type",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "是否支持预付费。返回值中 true 为支持、false 为不支持",
          "member": "bool",
          "name": "Prepayment",
          "required": true,
          "type": "bool",
          "value_allowed_null": false
        }
      ],
      "usage": "out"
    },
    "AvailableZone": {
      "document": "版本控制-可用区数组",
      "members": [
        {
          "default": "",
          "document": "可用区名称",
          "member": "string",
          "name": "Zone",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "可用区ID",
          "member": "int64",
          "name": "ZoneId",
          "required": true,
          "type": "int",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "可用区中文名称",
          "member": "string",
          "name": "ZoneCnName",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "Type数组",
          "member": "AvailableType",
          "name": "Types",
          "required": true,
          "type": "list",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "可用区中英文名称",
          "member": "string",
          "name": "ZoneName",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        }
      ],
      "usage": "out"
    },
    "CreateCfsFileSystemRequest": {
      "document": "CreateCfsFileSystem请求参数结构体",
      "members": [
        {
          "default": "",
          "document": "可用区名称，例如ap-beijing-1，请参考 [概览](https://cloud.tencent.com/document/product/582/13225) 文档中的地域与可用区列表",
          "example": "ap-beijing-1",
          "member": "string",
          "name": "Zone",
          "required": true,
          "type": "string"
        },
        {
          "default": "",
          "document": "网络类型，值为 VPC，BASIC；其中 VPC 为私有网络，BASIC 为基础网络",
          "example": "VPC",
          "member": "string",
          "name": "NetInterface",
          "required": true,
          "type": "string"
        },
        {
          "default": "",
          "document": "权限组 ID",
          "example": "pgroupbasic",
          "member": "string",
          "name": "PGroupId",
          "required": true,
          "type": "string"
        },
        {
          "default": "",
          "document": "文件系统协议类型， 值为 NFS、CIFS; 若留空则默认为 NFS协议",
          "example": "NFS",
          "member": "string",
          "name": "Protocol",
          "required": false,
          "type": "string"
        },
        {
          "default": "",
          "document": "文件系统存储类型，值为 SD ；其中 SD 为标准型存储， HP为性能存储。",
          "example": "SD",
          "member": "string",
          "name": "StorageType",
          "required": false,
          "type": "string"
        },
        {
          "default": "",
          "document": "私有网络（VPC） ID，若网络类型选择的是VPC，该字段为必填。",
          "example": "123",
          "member": "string",
          "name": "VpcId",
          "required": false,
          "type": "string"
        },
        {
          "default": "",
          "document": "子网 ID，若网络类型选择的是VPC，该字段为必填。",
          "example": "234",
          "member": "string",
          "name": "SubnetId",
          "required": false,
          "type": "string"
        },
        {
          "default": "",
          "document": "指定IP地址，仅VPC网络支持；若不填写、将在该子网下随机分配 IP",
          "example": "10.0.0.1",
          "member": "string",
          "name": "MountIP",
          "required": false,
          "type": "string"
        },
        {
          "default": "",
          "document": "用户自定义文件系统名称",
          "example": "QcloudCfs",
          "member": "string",
          "name": "FsName",
          "required": false,
          "type": "string"
        },
        {
          "default": "",
          "document": "文件系统标签",
          "example": "[\"TagKey\":\"a\",\"TagValue\":1]",
          "member": "TagInfo",
          "name": "ResourceTags",
          "required": false,
          "type": "list"
        },
        {
          "default": "",
          "document": "用于保证请求幂等性的字符串。该字符串由客户生成，需保证不同请求之间唯一，最大值不超过64个ASCII字符。若不指定该参数，则无法保证请求的幂等性。用于保证请求幂等性的字符串失效时间为2小时。",
          "example": "QcloudCfs001",
          "member": "string",
          "name": "ClientToken",
          "required": false,
          "type": "string"
        }
      ],
      "type": "object"
    },
    "CreateCfsFileSystemResponse": {
      "document": "CreateCfsFileSystem返回参数结构体",
      "members": [
        {
          "document": "文件系统创建时间",
          "example": "2017-02-24 11:28:12",
          "member": "string",
          "name": "CreationTime",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "document": "用户自定义文件系统名称",
          "example": "QcloudCfs",
          "member": "string",
          "name": "CreationToken",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "document": "文件系统 ID",
          "example": "cfs-abc",
          "member": "string",
          "name": "FileSystemId",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "document": "文件系统状态",
          "example": "AVAILABLE",
          "member": "string",
          "name": "LifeCycleState",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "document": "文件系统已使用容量大小",
          "example": "1000",
          "member": "uint64",
          "name": "SizeByte",
          "required": true,
          "type": "int",
          "value_allowed_null": false
        },
        {
          "document": "可用区 ID",
          "example": "800001",
          "member": "uint64",
          "name": "ZoneId",
          "required": true,
          "type": "int",
          "value_allowed_null": false
        },
        {
          "document": "用户自定义文件系统名称",
          "example": "QcloudCfs",
          "member": "string",
          "name": "FsName",
          "required": true,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "document": "文件系统是否加密",
          "example": "False",
          "member": "bool",
          "name": "Encrypted",
          "required": true,
          "type": "bool",
          "value_allowed_null": false
        },
        {
          "document": "唯一请求 ID，每次请求都会返回。定位问题时需要提供该次请求的 RequestId。",
          "member": "string",
          "name": "RequestId",
          "type": "string"
        }
      ],
      "type": "object"
    },
    "CreateCfsPGroupRequest": {
      "document": "CreateCfsPGroup请求参数结构体",
      "members": [
        {
          "default": "",
          "document": "权限组名称，1-64个字符且只能为中文，字母，数字，下划线或横线",
          "member": "string",
          "name": "Name",
          "required": true,
          "type": "string"
        },
        {
          "default": "",
          "document": "权限组描述信息，1-255个字符",
          "member": "string",
