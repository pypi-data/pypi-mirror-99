@CONFIG Version=2

# Torridon XLC Power Module + Triggering
@HEADER
DeviceClass=TorridonPowerModule
DeviceNumbers=QTL1824-01,QTL1824-02
DeviceNumbersExclude=
MinFirmwareRequired=4.210
MinFpgaRequired=1.2
DeviceDescription=XLC Power Margining Module + Triggering

#Describes general module abilities
@GENERAL
Logging_Present=true
Triggering_Present=true
Fixture_Version=1
OutputVoltageLimit_Present=true
MeasDumpVersion=2
PreTrigger_Present=true
MeasThreshTrigger_Present=true
OutputPullDown_Present=true
OutputCapacitance_Present = true
MemorySize_Present=524287
Graph_Display_Min=2
MultiRangeThresh_Present=true
TempWarningUnit=60
TempNearCutoffUnit=70
TempWarningFet=60
TempNearCutoffFet=70


@PORT_GROUPS
Name=ALL,Ports="12v,5v"

#Describes ports
@PORTS

@PORT_START
PortName=12v
Type=VARIABLE

Nominal_Voltage=12000
Voltage_Limits=int,0,14400,1
Current_Measure_Limits=int,0,7500,1

Pattern_Present=true
Pattern_Points=1023
Pattern_Time_Limits=int,0,2147483647,1
Pattern_Repeat_Limits=int,1,65535,1
PortColor=Red
@PORT_END

@PORT_START
PortName=5v
Type=VARIABLE

Nominal_Voltage=5000
Voltage_Limits=int,0,6000,1
Current_Measure_Limits=int,0,7500,1

Pattern_Present=true
Pattern_Points=1023
Pattern_Time_Limits=int,0,2147483647,1
Pattern_Repeat_Limits=int,1,65535,1
PortColor=Blue
@PORT_END

@LOGGING
USB_Live_Present=true
Live_Rate=1000
Logging_Channel_Order=5v,12v
Logging_Rate_Limits=int,1,10000,1



@MEASURE
Name=3v3,Type=SelfVoltage,Description=3.3v Internal
Name=6v5,Type=SelfVoltage,Description=6.5v Internal
Name=15v,Type=SelfVoltage,Description=15v Internal
Name=20v,Type=SelfVoltage,Description=20v Internal
Name=Unit,Type=SelfTemperature,Description=Unit Ambient
Name=5v,Type=SelfTemperature,Description=5v Regulator
Name=12v,Type=SelfTemperature,Description=12v Regulator
Name=5v,Type=Voltage,Description=5v Output Voltage
Name=12v,Type=Voltage,Description=12v Output Voltage
Name=5v,Type=Current,Description=5v Output Current
Name=12v,Type=Current,Description=12v Output Current
Name=5v,Type=Power,Description=5v Output Power
Name=12v,Type=Power,Description=12v Output Power