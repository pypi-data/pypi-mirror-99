<odoo>
  <data>

    <record id="view_cs_car_service_window" model="ir.actions.act_window">
      <field name="name">Carsharing car services</field>
      <field name="res_model">fleet.vehicle.log.services</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record id="view_cs_car_service_list" model="ir.ui.view">
      <field name="name">sm_carsharing_structure.cs_car_service.tree</field>
      <field name="model">fleet.vehicle.log.services</field>
      <field name="inherit_id" ref="fleet.fleet_vehicle_log_services_view_tree"/>
      <field name="arch" type="xml">
        <xpath expr="//tree"  position="replace">
          <tree>
            <field name="date" />
            <field name="vehicle_id" />
            <field name="cost_subtype_id"/>
            <field name="related_member_id" />
            <field name="vendor_id" />
            <field name="related_invoice_id" />
            <field name="amount" sum="Total"/>
          </tree>
        </xpath>
      </field>
    </record>
    <record id="view_cs_car_service_form" model="ir.ui.view">
      <field name="name">sm_carsharing_structure.cs_car_service.form</field>
      <field name="model">fleet.vehicle.log.services</field>
      <field name="type">form</field>
      <field name="inherit_id" ref="fleet.fleet_vehicle_log_services_view_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='date']"  position="after">
          <field name="related_member_id"/>
        </xpath>
        <xpath expr="//field[@name='vendor_id']"  position="after">
          <field name="related_invoice_id"/>
          <field name="related_task_id"/>
        </xpath>
        <xpath expr="//field[@name='inv_ref']"  position="replace"></xpath>
        <xpath expr="//field[@name='purchaser_id']"  position="replace"></xpath>
      </field>
    </record>

    <menuitem name="CS Car services" id="sm_carsharing_structure.cs_car_service" parent="sm_carsharing_structure.menu_root"
          action="view_cs_car_service_window"/>

  </data>
</odoo>