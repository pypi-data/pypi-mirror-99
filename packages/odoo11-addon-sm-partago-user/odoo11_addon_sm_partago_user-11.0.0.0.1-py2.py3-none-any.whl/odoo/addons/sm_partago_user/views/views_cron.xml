<odoo>
  <data noupdate="1">
    <record id="ir_cron_check_completed_form" model="ir.cron">
      <field name="name">Fetch and complete cs update data</field>
      <field eval="False" name="active"/>
      <field name="user_id" ref="base.user_root"/>
      <field name="interval_number">1</field>
      <field name="interval_type">hours</field>
      <field name="numbercall">-1</field>
      <field name="model_id" ref="model_sm_partago_user_smp_user_cron"/>
      <field name="state">code</field>
      <field name="code">model.fetch_and_complete_cs_update_data_from_wp()</field>
    </record>
  </data>
  <data noupdate="1">
  <record id="ir_cron_complete_cs_registrations" model="ir.cron">
      <field name="name">Complete cs registrations</field>
      <field eval="False" name="active"/>
      <field name="user_id" ref="base.user_root"/>
      <field name="interval_number">1</field>
      <field name="interval_type">hours</field>
      <field name="numbercall">-1</field>
      <field name="model_id" ref="model_sm_partago_user_smp_user_cron"/>
      <field name="state">code</field>
      <field name="code">model.complete_cs_registrations()</field>
    </record>
  </data>
  <data noupdate="1">
    <record id="ir_cron_fetch_cs_groups_form" model="ir.cron">
      <field name="name">Fetch cs member groups</field>
      <field eval="False" name="active"/>
      <field name="user_id" ref="base.user_root"/>
      <field name="interval_number">1</field>
      <field name="interval_type">hours</field>
      <field name="numbercall">-1</field>
      <field name="model_id" ref="model_sm_partago_user_smp_user_cron"/>
      <field name="state">code</field>
      <field name="code">model.auto_update_member_cs_groups()</field>
    </record>
  </data>

  <data noupdate="1">
    <record id="ir_cron_member_cs_registration_status" model="ir.cron">
      <field name="name">Update members cs registration status</field>
      <field eval="False" name="active"/>
      <field name="user_id" ref="base.user_root"/>
      <field name="interval_number">1</field>
      <field name="interval_type">days</field>
      <field name="numbercall">-1</field>
      <field name="model_id" ref="model_sm_partago_user_smp_user_cron"/>
      <field name="state">code</field>
      <field name="code">model.update_members_carsharing_registration_status()</field>
    </record>
  </data>
</odoo>
