<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="cs_user_res_config_settings_view_form" model="ir.ui.view">
    <field name="name">cs_user.res.config.settings.view.form.inherit.base.setup</field>
    <field name="model">res.config.settings</field>
    <field name="priority" eval="100"/>
    <field name="inherit_id" ref="base.res_config_settings_view_form"/>
    <field name="arch" type="xml">
      <xpath expr="//div[hasclass('settings')]" position="inside">
        <div class="app_settings_block" data-string="CS User" string="CS User" data-key="cs_user">
          <div class="sm_mail_templates">
            <h2>Mail Templates</h2>
            <div class="row mt16 o_settings_container">
              <group>
                <field name="cs_already_active"/>
                <field name="cs_already_requested_access"/>
                <field name="cs_missing_data_email_template_id"/>
                <field name="cs_company_access_already_requested"/>
                <field name="cs_complete_data_soci_not_found_email_template_id"/>
                <field name="cs_complete_data_successful_email_template_id"/>
              </group>
            </div>
          </div>
        </div>
      </xpath>
    </field>
  </record>
</odoo>