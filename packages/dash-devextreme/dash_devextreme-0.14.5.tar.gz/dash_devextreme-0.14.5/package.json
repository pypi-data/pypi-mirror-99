{
	"name": "dash-devextreme",
	"version": "0.14.5",
	"description": "Wrapper of DevExtreme components for Plotly Dash",
	"main": "build/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/pikhovkin/dash-devextreme.git"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/pikhovkin/dash-devextreme/issues"
	},
	"homepage": "https://github.com/pikhovkin/dash-devextreme",
	"keywords": [
		"dash",
		"plotly",
		"plotly-dash",
		"devextreme"
	],
	"author": "Sergei Pikhovkin <s@pikhovkin.ru>",
	"scripts": {
		"start": "webpack-serve --config ./webpack.serve.config.js --open",
		"validate-init": "python _validate_init.py",
		"prepublishOnly": "npm run validate-init",
		"build:js": "webpack --mode production",
		"build:js_dev": "webpack --mode development",
		"build:py": "dash-generate-components ./src/lib/components dash_devextreme -p package-info.json",
		"build:py_and_r": "dash-generate-components ./src/lib/components dash_devextreme -p package-info.json --r-prefix '' --jl-prefix ''",
		"build:py_and_r-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
		"build": "npm run build:js && npm run build:py_and_r",
		"build:dev": "npm run build:js_dev && npm run build:py",
		"build:activated": "npm run build:js && npm run build:py_and_r-activated"
	},
	"dependencies": {
		"devextreme": "20.2.4",
		"devextreme-react": "20.2.4",
		"ramda": "^0.26.1"
	},
	"devDependencies": {
		"@babel/core": "^7.5.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.4",
		"@babel/preset-env": "^7.5.4",
		"@babel/preset-react": "^7.0.0",
		"babel-eslint": "^10.0.2",
		"babel-loader": "^8.0.6",
		"copyfiles": "^2.1.1",
		"css-loader": "^3.0.0",
		"eslint": "^6.0.1",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-import": "^2.18.0",
		"eslint-plugin-react": "^7.14.2",
		"npm": "^6.1.0",
		"prop-types": "^15.7.2",
		"react": "^16.8.6",
		"react-docgen": "^4.1.1",
		"react-dom": "^16.8.6",
		"styled-jsx": "^3.2.1",
		"style-loader": "^0.23.1",
		"webpack": "4.36.1",
		"webpack-cli": "3.3.6",
		"webpack-serve": "3.1.0"
	},
	"engines": {
		"node": ">=8.11.0",
		"npm": ">=6.1.0"
	}
}
