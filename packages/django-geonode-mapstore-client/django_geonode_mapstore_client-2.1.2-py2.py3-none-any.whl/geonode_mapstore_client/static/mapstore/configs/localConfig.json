                                "container": "#footer-attribution-container"
                            },
                            "interactions": {
                                "pinchRotate": false,
                                "altShiftDragRotate": false
                            }
                        }
                    }
                }
            },
            {
                "name": "Identify",
                "cfg": {
                    "viewerOptions": {
                        "container": "{context.ReactSwipe}"
                    }
                },
                "override": {
                    "Toolbar": {
                        "position": 11,
                        "alwaysVisible": false
                    }
                }
