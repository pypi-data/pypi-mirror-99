<odoo>
  <data>
    <report
        id="sm_report_data.mossos_report"
        model="sm_report_data.sm_report_data"
        string="Print to XLSX"
        report_type="xlsx"
        name="sm_report_data.mossos"
        file="sm_report_data_sm_report_data"
        attachment_use="True"
    />
  </data>

  <data noupdate="1">
    <record id="ir_cron_smp_user_scheduler_action" model="ir.cron">
      <field name="name">Reports cron</field>
      <field eval="False" name="active"/>
      <field name="user_id" ref="base.user_root"/>
      <field name="interval_number">24</field>
      <field name="interval_type">hours</field>
      <field name="numbercall">0</field>
      <field name="model_id" ref="model_sm_report_data_reports_cron"/>
      <field name="state">code</field>
      <field name="code">model.reports_cron_action()</field>
    </record>
  </data>
</odoo>
