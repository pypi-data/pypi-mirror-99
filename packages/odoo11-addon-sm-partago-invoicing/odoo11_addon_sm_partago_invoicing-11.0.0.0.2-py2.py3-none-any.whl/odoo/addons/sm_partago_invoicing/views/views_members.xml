<odoo>
  <data>
    <record id="view_members_tariff_cs_monthly_fee_form" model="ir.ui.view">
      <field name="name">sm_partago_invoicing.members_tariff_cs_monthly_fee.form</field>
      <field name="model">res.partner</field>
      <field name="type">form</field>
      <field name="inherit_id" ref="sm_partago_tariffs.view_tariff_members_form"/>
      <field name="arch" type="xml">
        <xpath expr="//notebook/page[@name='carsharing']" position="inside">
          <group>
            <field name="cs_monthly_fee"/>
            <field name="cs_monthly_fee_type"/>
            <field name="paymaster"/>
            <field name="invoicing_email" widget="email"/>
          </group>
        </xpath>
      </field>
    </record>
  </data>
</odoo>
