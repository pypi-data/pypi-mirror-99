
<odoo>
  <data>
    <record id="view_report_reservation_compute_form" model="ir.ui.view">
      <field name="name">smp.report_reservation_compute.form</field>
      <field name="model">smp.sm_report_reservation_compute</field>
      <field name="type">form</field>
      <field name="arch" type="xml">
        <form string="Edit payment" version="7.0" delete="false">
          <sheet>
            <notebook>
              <page string="General">
                <group>
                  <field name="name" attrs="{'required': True}"/>
                  <field name="report_type" attrs="{'required': True}"/>
                  <field name="member_id" attrs="{'readonly': True}"/>
                  <field name="invoice_report_id"/>
                  <field name="invoice_id"/>
                  <field name="report_total_no_discounts"/>
                  <field name="report_discount"/>
                  <field name="report_total"/>
                  <field name="invoice_total"/>
                  <field name="invoice_taxes"/>
                  <field name="reservation_computes_id" attrs="{'readonly': True}"/>
                  <field name="teletacs_compute_id" attrs="{'readonly': True}"/>
                </group>
              </page>
            </notebook>
          </sheet>
        </form>
      </field>
    </record>
  </data>
</odoo>