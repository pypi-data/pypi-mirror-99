<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="view_teletac_in_invoice_report_form" model="ir.ui.view">
      <field name="name">sm_invocies_reports.sm_teletac.form.inherit</field>
      <field name="model">smp.sm_teletac</field>
      <field name="inherit_id" ref="sm_teletacs.view_teletac_form"/>
      <field name="arch" type="xml">
        <xpath expr="//page/group" position="inside">
          <field name="invoice_report_id"/>
        </xpath>
      </field>
    </record>
    <record id="view_teletac_in_invoice_report_list" model="ir.ui.view">
      <field name="name">sm_invocies_reports.sm_teletac.list.inherit</field>
      <field name="model">smp.sm_teletac</field>
      <field name="inherit_id" ref="sm_teletacs.view_teletac_list"/>
      <field name="arch" type="xml">
        <xpath expr="//tree" position="inside">
          <field name="invoice_report_id"/>
        </xpath>
      </field>
    </record>
  </data>
</odoo>
