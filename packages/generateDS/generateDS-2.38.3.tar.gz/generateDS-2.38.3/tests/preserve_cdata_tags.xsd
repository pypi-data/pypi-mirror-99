<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.example.org/wheel/" targetNamespace="http://www.example.org/wheel/">


    <element name="Wheel" type="tns:Wheel"></element>

    <complexType name="Wheel">
        <sequence>
            <element name="description" type="string"></element>
            <element name="radius">
                <simpleType>
                    <restriction base="float">
                        <minInclusive value="0"></minInclusive>
                    </restriction>
                </simpleType>
            </element>
        </sequence>
    </complexType>
</schema>
