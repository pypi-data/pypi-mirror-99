<odoo>
  <data>

    <record id="view_fetch_sm_wp_db_wizard" model="ir.ui.view">
      <field name="name">smp.fetch_sm_wp_db_wizard.form</field>
      <field name="model">sommobilitat.sm_member_fetch_wizard</field>
      <field name="arch" type="xml">
        <form string="Are you sure?">
          <footer>
            <button name="create_request"
              string="Yes" type="object"
              class="btn-primary"/>
            <button string="No"
              class="btn-default"
              special="cancel"/>
          </footer>
        </form>
      </field>
    </record>

    <record id="action_view_fetch_sm_wp_db_wizard" model="ir.actions.act_window">
      <field name="name">Are you sure?</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">sommobilitat.sm_member_fetch_wizard</field>
      <field name="view_type">form</field>
      <field name="view_mode">form</field>
      <field name="view_id" ref="view_fetch_sm_wp_db_wizard"/>
      <field name="target">new</field>
    </record>

    <menuitem name="DB: Fetch www members" id="sommobilitat.sm_member_fetch_wizard" 
      parent="sommobilitat.users" action="action_view_fetch_sm_wp_db_wizard" sequence="11"/>

  </data>
</odoo>
