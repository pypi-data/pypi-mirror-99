<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="invoices_journal" model="account.journal">
      <field name="name">Customer Invoices Journal</field>
      <field name="code">INV</field>
      <field name="type">sale</field>
      <field name="refund_sequence" eval="True"></field>
      <field name="update_posted" eval="True"></field>
      <field name="default_debit_account_id" ref="l10n_es.1_account_common_7050"/>
      <field name="default_credit_account_id" ref="l10n_es.1_account_common_7050"/>
      <field name="sequence">10</field>
    </record>
    <record id="consumption_invoices_journal" model="account.journal">
      <field name="name">Consumptions Invoices Journal</field>
      <field name="code">CON</field>
      <field name="type">sale</field>
      <field name="refund_sequence" eval="True"></field>
      <field name="update_posted" eval="True"></field>
      <field name="default_debit_account_id" ref="l10n_es.1_account_common_7050"/>
      <field name="default_credit_account_id" ref="l10n_es.1_account_common_7050"/>
      <field name="sequence">11</field>
    </record>
    <record id="subscription_journal" model="account.journal">
      <field name="name">SC Subscription Journal</field>
      <field name="code">SUBJ</field>
      <field name="type">sale</field>
      <field name="sequence_id" ref="easy_my_coop.sequence_subscription_journal"/>
