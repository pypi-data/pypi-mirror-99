      <field name="default_debit_account_id" ref="l10n_es.1_account_common_600"/>
      <field name="default_credit_account_id" ref="l10n_es.1_account_common_600"/>
    </record>
    <record id="caixa_guissona_journal" model="account.journal">
      <field name="name">Caixa Guissona</field>
      <field name="code">BGUI</field>
