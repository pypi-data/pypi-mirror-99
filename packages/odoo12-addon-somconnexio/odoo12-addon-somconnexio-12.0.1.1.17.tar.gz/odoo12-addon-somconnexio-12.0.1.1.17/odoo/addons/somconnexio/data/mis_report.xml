<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="mis_report_trial_balance" model="mis.report">
      <field name="name">Trial balance</field>
      <field name="style_id" ref="l10n_es_mis_report.mis_report_style_l10n_es_base"/>
    </record>
    <record id="mis_report_subkpi_init" model="mis.report.subkpi">
      <field name="name">init</field>
      <field name="description">Initial</field>
      <field name="report_id" ref="mis_report_trial_balance"/>
    </record>
    <record id="mis_report_subkpi_debit" model="mis.report.subkpi">
      <field name="name">debit</field>
      <field name="description">Debit</field>
      <field name="report_id" ref="mis_report_trial_balance"/>
    </record>
    <record id="mis_report_subkpi_credit" model="mis.report.subkpi">
      <field name="name">credit</field>
      <field name="description">Credit</field>
      <field name="report_id" ref="mis_report_trial_balance"/>
    </record>
    <record id="mis_report_subkpi_end" model="mis.report.subkpi">
      <field name="name">end</field>
      <field name="description">End</field>
      <field name="report_id" ref="mis_report_trial_balance"/>
    </record>
    <record id="mis_report_kpi_balance" model="mis.report.kpi">
      <field name="name">balance</field>
      <field name="description">Balance</field>
      <field name="report_id" ref="mis_report_trial_balance"/>
      <field name="multi" eval="True"/>
    </record>
    <record id="mis_report_kpi_expression_balance_init" model="mis.report.kpi.expression">
      <field name="name">bali[]</field>
      <field name="kpi_id" ref="mis_report_kpi_balance"/>
      <field name="subkpi_id" ref="mis_report_subkpi_init"/>
    </record>
    <record id="mis_report_kpi_expression_balance_end" model="mis.report.kpi.expression">
      <field name="name">bale[]</field>
      <field name="kpi_id" ref="mis_report_kpi_balance"/>
      <field name="subkpi_id" ref="mis_report_subkpi_end"/>
    </record>
    <record id="mis_report_kpi_expression_balance_credit" model="mis.report.kpi.expression">
      <field name="name">crdp[]</field>
      <field name="kpi_id" ref="mis_report_kpi_balance"/>
      <field name="subkpi_id" ref="mis_report_subkpi_credit"/>
    </record>
    <record id="mis_report_kpi_expression_balance_debit" model="mis.report.kpi.expression">
      <field name="name">debp[]</field>
      <field name="kpi_id" ref="mis_report_kpi_balance"/>
      <field name="subkpi_id" ref="mis_report_subkpi_debit"/>
    </record>
  </data>
</odoo>
