<?xml version="1.0" encoding="utf-8" ?>
<odoo>
  <record id="product_category_technology_supplier_view" model="ir.ui.view">
    <field name="name">product.category.technology.supplier.tree</field>
    <field name="model">product.category.technology.supplier</field>
    <field name="arch" type="xml">
      <tree editable="bottom" create="true">
        <field name="product_category_id"/>
        <field name="service_technology_id"/>
        <field name="service_supplier_id"/>
      </tree>
    </field>
  </record>
  <record id="product_category_technology_supplier_action"  model="ir.actions.act_window">
    <field name="name">Product Category - Service Technology - Service Supplier relation</field>
    <field name="res_model">product.category.technology.supplier</field>
    <field name="view_type">form</field>
    <field name="view_mode">tree</field>
  </record>
</odoo>
