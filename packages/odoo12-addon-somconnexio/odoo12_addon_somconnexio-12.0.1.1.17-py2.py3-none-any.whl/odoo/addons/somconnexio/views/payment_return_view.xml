<?xml version="1.0" encoding="utf-8" ?>
<odoo>
  <record id="payment_return_form_view" model="ir.ui.view">
    <field name="name">payment.return.form</field>
    <field name="model">payment.return</field>
    <field name="inherit_id" ref="account_payment_return.payment_return_form_view" />
    <field name="arch" type="xml">
      <field name="journal_id" position="attributes">
        <attribute name="domain"/>
      </field>
      <field name="move_line_ids" position="attributes">
        <attribute name="domain">[
          ('partner_id', '=', partner_id),
          ('journal_id', '=', parent.journal_id),
          ('account_id.internal_type', '=', 'receivable')
        ]</attribute>
      </field>
    </field>
  </record>
</odoo>
