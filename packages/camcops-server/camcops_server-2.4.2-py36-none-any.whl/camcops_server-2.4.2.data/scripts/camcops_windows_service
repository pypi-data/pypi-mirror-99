#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'camcops-server==2.4.2','console_scripts','camcops_windows_service'
__requires__ = 'camcops-server==2.4.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('camcops-server==2.4.2', 'console_scripts', 'camcops_windows_service')()
    )
