{
    "connection": {
        "type": {
            "displayName": "Async Sample",
            "type": "connectorType"
        },
        "host": {
            "type": "text"
        },
        "port": {
            "type": "number",
            "default": 443
        },
        "help": {
            "type": "link",
            "default": "https://www.ibm.com/support/knowledgecenter/SSTDPP_1.6.0/platform/docs/scp-core/data-sources.html"
        },
        "sni": {
            "type": "text",
            "optional": true
        },
        "selfSignedCert": {
            "type": "password",
            "optional": true
        },
        "options": {
            "type": "fields",
            "result_limit": {
                "default": 10000,
                "min": 1,
                "type": "number",
                "previous": "connection.resultSizeLimit"
            },
            "time_range": {
                "default": 5,
                "min": 1,
                "type": "number",
                "previous": "connection.timerange",
                "nullable": true
            },
            "timeout": {
                "default": 10,
                "min": 1,
                "max": 60,
                "hidden": true,
                "type": "number",
                "previous": "connection.timeoutLimit"
            },
            "dialects": {
                "type": "array",
                "hidden": true,
                "optional": true
            },
            "language": {
                "type": "string",
                "default": "stix",
                "optional": true,
                "hidden": true
            },
            "validate_pattern": {
                "type": "boolean",
                "optional": true,
                "hidden": true,
                "previous": "connection.validate_pattern"
            },
            "stix_validator": {
                "type": "boolean",
                "default": false,
                "optional": true,
                "hidden": true,
                "previous": "connection.stix_validator"
            },
            "mapping": {
                "type": "json",
                "optional": true,
                "hidden": true,
                "previous": "connection.mapping",
                "default": {
                    "dialect1_from_stix_map": {
                        "ipv4-addr": {
                            "fields": {
                                "value": [
                                    "SourceIpV4-dialect1",
                                    "DestinationIpV4-dialect1"
                                ]
                            }
                        },
                        "ipv6-addr": {
                            "fields": {
                                "value": [
                                    "SourceIpV6-dialect1",
                                    "DestinationIpV6-dialect1"
                                ]
                            }
                        },
                        "url": {
                            "fields": {
                                "value": [
                                    "Url-dialect1"
                                ]
                            }
                        },
                        "mac-addr": {
                            "fields": {
                                "value": [
                                    "SourceMac-dialect1",
                                    "DestinationMac-dialect1"
                                ]
                            }
                        },
                        "file": {
                            "fields": {
                                "name": [
                                    "FileName-dialect1"
                                ]
                            }
                        },
                        "network-traffic": {
                            "fields": {
                                "src_port": [
                                    "SourcePort-dialect1"
                                ],
                                "dst_port": [
                                    "DestinationPort-dialect1"
                                ],
                                "start": [
                                    "StartTime-dialect1"
                                ],
                                "end": [
                                    "EndTime-dialect1"
                                ],
                                "protocols[*]": [
                                    "NetworkProtocol-dialect1"
                                ],
                                "src_ref.value": [
                                    "SourceIpV4-dialect1",
                                    "SourceIpV6-dialect1"
                                ],
                                "dst_ref.value": [
                                    "DestinationIpV4-dialect1",
                                    "DestinationIpV6-dialect1"
                                ]
                            }
                        },
                        "user-account": {
                            "fields": {
                                "user_id": [
                                    "UserName-dialect1"
                                ]
                            }
                        },
                        "artifact": {
                            "fields": {
                                "payload_bin": [
                                    "Payload-dialect1"
                                ]
                            }
                        },
                        "domain-name": {
                            "fields": {
                                "value": [
                                    "DomainName-dialect1"
                                ]
                            }
                        }
                    },
                    "dialect2_from_stix_map": {
                        "ipv4-addr": {
                            "fields": {
                                "value": [
                                    "SourceIpV4-dialect2",
                                    "DestinationIpV4-dialect2"
                                ]
                            }
                        },
                        "ipv6-addr": {
                            "fields": {
                                "value": [
                                    "SourceIpV6-dialect2",
                                    "DestinationIpV6-dialect2"
                                ]
                            }
                        },
                        "url": {
                            "fields": {
                                "value": [
                                    "Url-dialect2"
                                ]
                            }
                        },
                        "mac-addr": {
                            "fields": {
                                "value": [
                                    "SourceMac-dialect2",
                                    "DestinationMac-dialect2"
                                ]
                            }
                        },
                        "file": {
                            "fields": {
                                "name": [
                                    "FileName-dialect2"
                                ]
                            }
                        },
                        "network-traffic": {
                            "fields": {
                                "src_port": [
                                    "SourcePort-dialect2"
                                ],
                                "dst_port": [
                                    "DestinationPort-dialect2"
                                ],
                                "start": [
                                    "StartTime-dialect2"
                                ],
                                "end": [
                                    "EndTime-dialect2"
                                ],
                                "protocols[*]": [
                                    "NetworkProtocol-dialect2"
                                ],
                                "src_ref.value": [
                                    "SourceIpV4-dialect2",
                                    "SourceIpV6-dialect2"
                                ],
                                "dst_ref.value": [
                                    "DestinationIpV4-dialect2",
                                    "DestinationIpV6-dialect2"
                                ]
                            }
                        },
                        "user-account": {
                            "fields": {
                                "user_id": [
                                    "UserName-dialect2"
                                ]
                            }
                        },
                        "artifact": {
                            "fields": {
                                "payload_bin": [
                                    "Payload-dialect2"
                                ]
                            }
                        },
                        "domain-name": {
                            "fields": {
                                "value": [
                                    "DomainName-dialect2"
                                ]
                            }
                        }
                    },
                    "to_stix_map": {
                        "UserName": {
                            "key": "user-account.user_id"
                        },
                        "LogSourceId": {
                            "key": "x__custom_property.log_source_id",
                            "cybox": false
                        },
                        "Magnitude": {
                            "key": "x__custom_property.magnitude",
                            "cybox": false
                        },
                        "SourceIpV4": [
                            {
                                "key": "ipv4-addr.value",
                                "object": "src_ip"
                            },
                            {
                                "key": "network-traffic.src_ref",
                                "object": "nt",
                                "references": "src_ip"
                            }
                        ],
                        "DestinationIpV4": [
                            {
                                "key": "ipv4-addr.value",
                                "object": "dst_ip"
                            },
                            {
                                "key": "network-traffic.dst_ref",
                                "object": "nt",
                                "references": "dst_ip"
                            }
                        ],
                        "SourceIpV6": [
                            {
                                "key": "ipv6-addr.value",
                                "object": "src_ip"
                            },
                            {
                                "key": "network-traffic.src_ref",
                                "object": "nt",
                                "references": "src_ip"
                            }
                        ],
                        "DestinationIpV6": [
                            {
                                "key": "ipv6-addr.value",
                                "object": "dst_ip"
                            },
                            {
                                "key": "network-traffic.dst_ref",
                                "object": "nt",
                                "references": "dst_ip"
                            }
                        ],
                        "EventCount": {
                            "key": "number_observed",
                            "cybox": false,
                            "transformer": "ToInteger"
                        },
                        "StartTime": [
                            {
                                "key": "first_observed",
                                "transformer": "EpochToTimestamp",
                                "cybox": false
                            },
                            {
                                "key": "last_observed",
                                "transformer": "EpochToTimestamp",
                                "cybox": false
                            }
                        ],
                        "Url": {
                            "key": "url.value"
                        },
                        "FileName": {
                            "key": "file.name"
                        },
                        "Payload": {
                            "key": "artifact.payload_bin"
                        },
                        "DestinationPort": {
                            "key": "network-traffic.dst_port",
                            "object": "nt",
                            "transformer": "ToInteger"
                        },
                        "SourcePort": {
                            "key": "network-traffic.src_port",
                            "object": "nt",
                            "transformer": "ToInteger"
                        },
                        "NetworkProtocol": {
                            "key": "network-traffic.protocols",
                            "object": "nt",
                            "transformer": "ToLowercaseArray"
                        },
                        "DomainName": {
                            "key": "domain-name.value"
                        },
                        "Process": {
                            "Path": [
                                {
                                    "object": "proc",
                                    "key": "process.command_line"
                                }
                            ],
                            "Pid": [
                                {
                                    "object": "proc",
                                    "key": "process.id"
                                }
                            ]
                        }
                    }
                }
            },
            "unmapped_fallback": {
                "type": "boolean",
                "default": false,
                "optional": true,
                "hidden": true
            }
        }
    },
    "configuration": {
        "type": "fields",
        "auth": {
            "username": {
                "type": "password"
            },
            "password": {
                "type": "password"
            }
        }
    }
}