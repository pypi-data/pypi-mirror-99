<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">

<body>

<metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main"
                    tal:define="dummy here/@@tpcheck/auth_modify_portal_content">

        <div tal:replace="structure provider:plone.abovecontenttitle" />

        <h1 class="documentFirstHeading"
            i18n:translate="">
            Functions of
            <tal:block tal:content="structure here/Title"
                       i18n:name="value"/>
        </h1>

        <div tal:replace="structure provider:plone.belowcontenttitle" />

        <div class="well">
            <h3 i18n:translate="">Functions</h3>
            <br/>
            <div class="field">
                <label tal:condition="here/getFile | nothing"
                       i18n:translate="">
                    OGG
                </label>
                <div>
                    <a tal:attributes="href string:${here/absolute_url}/@@unitraccvideo/update_ogg"
                       i18n:translate="">
                        Click here to update.
                    </a>
                </div>
            </div>

        </div>


    </tal:main-macro>
</metal:main>

</body>
</html>
