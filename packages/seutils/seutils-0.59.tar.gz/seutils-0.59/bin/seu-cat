#!/usr/bin/env python
# -*- coding: utf-8 -*-
import seutils
import argparse
parser = argparse.ArgumentParser()
parser.add_argument(
    'lfn', type=str,
    help='Path of file to be cat\'ed.'
    )
args = parser.parse_args()

def main():
    lfn = seutils.cli_flexible_format(args.lfn, seutils.cli_detect_fnal())
    output = seutils.cat(lfn)
    print(output)


if __name__ == '__main__':
    main()