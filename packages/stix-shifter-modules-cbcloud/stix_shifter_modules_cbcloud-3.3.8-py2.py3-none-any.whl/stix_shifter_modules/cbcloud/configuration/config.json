                            "object": "parent_file"
                        },
                        "parent_cmdline": {
                            "key": "process.command_line",
                            "object": "parent"
                        },
                        "parent_path": [
                            {
                                "key": "directory.path",
                                "object": "parent_dir"
                            },
                            {
                                "key": "file.parent_directory_ref",
                                "object": "parent_file",
                                "references": "parent_dir"
                            }
                        ],
