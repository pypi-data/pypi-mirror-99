<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="vertical_cs_res_config_settings_view_form" model="ir.ui.view">
    <field name="name">vertical_cs.res.config.settings.view.form.inherit.base.setup</field>
    <field name="model">res.config.settings</field>
    <field name="priority" eval="100"/>
    <field name="inherit_id" ref="base.res_config_settings_view_form"/>
    <field name="arch" type="xml">
      <xpath expr="//div[hasclass('settings')]" position="inside">
        <div class="app_settings_block" data-string="CS Accounting" string="CS Accounting" data-key="cs_accounting">
          <div id="sm_invoice_params_config">
            <h2>CS Accounting-Invoicing</h2>
            <div class="row mt16 o_settings_container">
              <div class="col-xs-12 o_setting_box">
                <div class="o_setting_right_pane">
                  <group>
                      <field name="cs_invoice_payment_mode_id"/>
                      <field name="notfound_car_analytic_account_id"/>
                      <field name="notfound_teletac_analytic_account_id"/>
                  </group>
                </div>
              </div>
            </div>
          </div>
        </div>
      </xpath>
    </field>
  </record>
</odoo>