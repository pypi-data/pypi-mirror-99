{
        "ociVersion": "1.0.0",
        "process": {
                "terminal": true,
                "user": {
                        "uid": 0,
                        "gid": 0
                },
                "args": [
                        "/bin/sh"
                ],
                "env": ["PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
                        "TERM=xterm"
                ],
                "cwd": "/",
                "noNewPrivileges": true
        },
        "root": {
                "path": ".",
                "readonly": true
        },
        "hostname": "runc",
        "linux": {
                "uidMappings": [
                        {
                                "hostID": 1000,
                                "containerID": 0,
                                "size": 1
                        }
                ],
                "gidMappings": [
                        {
                                "hostID": 1000,
                                "containerID": 0,
                                "size": 1
                        }
                ]
        }
}
