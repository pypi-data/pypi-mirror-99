<odoo>
  <data>

    <record id="view_registration_request_list_inherit" model="ir.ui.view">
      <field name="name">sm_partago_user.registration_request.tree.inherit</field>
      <field name="model">sm_partago_user.carsharing_registration_request</field>
      <field name="inherit_id" ref="sm_partago_user.view_registration_request_list"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='related_cs_update_data_id']"  position="after">
          <field name="related_reward_id" />
        </xpath>
      </field>
    </record>
    <record id="view_registration_request_form_inherit" model="ir.ui.view">
      <field name="name">sm_partago_user.registration_request.form.inherit</field>
      <field name="model">sm_partago_user.carsharing_registration_request</field>
      <field name="type">form</field>
      <field name="inherit_id" ref="sm_partago_user.view_registration_request_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='related_cs_update_data_id']"  position="after">
          <field name="related_reward_id"/>
        </xpath>
      </field>
    </record>

  </data>
</odoo>