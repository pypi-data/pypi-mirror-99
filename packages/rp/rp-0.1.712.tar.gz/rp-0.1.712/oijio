•'|'
'd|'
'de|'
'def|'
'def |' ⟶ 'def |():'
'def f|():'
    •'def f |():' ⟶ 'def f(|):'
        •'def f(⮐|):' ⟶ 'def f():⮐'
        •'def f(x|):'
            • 'def f(x |):' ⟶ 'def f(x,|):'
            • 'def f(x ⮐|):' ⟶ 'def f(x):⮐'
    •'def f⮐|():' ⟶ 'def f():⮐'


•'|'
'lambda|'
'lambda |' ⟶ 'lambda |:'
    •'lambda  |:' ⟶ 'lambda:|'
    •'lambda x|:'
    'lambda x |:' ⟶ 'lambda x,|:'
        •'lambda x, |:' ⟶ 'lambda x:|'


