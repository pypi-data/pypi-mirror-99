{
  "name": "widget-bandsplot",
  "version": "0.1.0",
  "description": "A Jupyter widget to plot bandstructures.",
  "author": "Dou Du",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/osscar/widget-bandsplot.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/ && rimraf ../widget_bandsplot/labextension/ && rimraf ../widget_bandsplot/nbextension",
    "prepublish": "yarn run clean && yarn run build:prod",
    "build": "webpack --mode=development && yarn run build:labextension:dev",
    "build:prod": "webpack --mode=production && yarn run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.4",
    "rimraf": "^2.6.1",
    "webpack": "^5"
  },
  "dependencies": {
    "@angular/material": "10.2.2",
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3 || ^4",
    "bootstrap": "3.3.7",
    "chart.js": "2.9.4",
    "chartjs-plugin-zoom": "0.7.7",
    "chartjs-plugin-annotation": "0.5.7",
    "jquery": "3.5.1",
    "lodash": "^4.17.4",
    "tinycolor2": "1.4.2",
    "underscore": "1.12.0"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../widget_bandsplot/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
